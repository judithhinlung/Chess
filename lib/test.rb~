require 'json'

class Person
  attr_accessor :name, :age
  def initialize(name, age)
    @name = name
    @age = age
  end
end
class Teacher < Person
  attr_accessor  :grade_level
  def initialize(name, age, grade_level)
    super(name, age)
        @grade_level = grade_level
  end
end
a = [[1, 2, 3], [4, 5, 6], [7, 8, 9]]
new_a = Array.new(3) { Array.new(3) }
a.each_with_index do |row, row_index|
  row.each_with_index do |col, col_index|
    new_a[row_index][col_index] = col
  end
end
new_a.each do |row|
  row.each do |col|
    print col
  end
  puts
end

